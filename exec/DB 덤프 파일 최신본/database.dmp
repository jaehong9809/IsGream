CREATE TABLE `htp_test` (
  `htp_test_id` int NOT NULL AUTO_INCREMENT,
  `child_id` int NOT NULL,
  `test_date` date NOT NULL DEFAULT ((now() + interval 0 day)),
  `house_drawing_url` varchar(1024) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `tree_drawing_url` varchar(1024) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `analysis_result` text COLLATE utf8mb4_unicode_ci,
  `pdf_url` varchar(1024) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `emoji` tinyint DEFAULT NULL,
  `created_at` datetime(6) NOT NULL,
  `updated_at` datetime(6) NOT NULL,
  `male_drawing_url` varchar(1024) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `female_drawing_url` varchar(1024) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `user_id` int NOT NULL,
  PRIMARY KEY (`htp_test_id`),
  KEY `htp_test_ibfk_1` (`child_id`),
  CONSTRAINT `htp_test_ibfk_1` FOREIGN KEY (`child_id`) REFERENCES `child` (`child_id`) ON DELETE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=240 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
